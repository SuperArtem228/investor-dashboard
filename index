    <script>
        // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –ø–µ—Ä–µ–º–µ–Ω–Ω—ã—Ö
        let currentFilter = 'all';
        
        // –§—É–Ω–∫—Ü–∏—è –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏—è –≤–∫–ª–∞–¥–æ–∫
        function switchTab(tabName) {
            // –°–∫—Ä—ã—Ç—å –≤—Å–µ —Å—Ç—Ä–∞–Ω–∏—Ü—ã
            document.querySelectorAll('.page').forEach(page => {
                page.classList.remove('active');
            });
            
            // –£–±—Ä–∞—Ç—å –∞–∫—Ç–∏–≤–Ω—ã–π –∫–ª–∞—Å—Å —Å–æ –≤—Å–µ—Ö –≤–∫–ª–∞–¥–æ–∫
            document.querySelectorAll('.nav-tab').forEach(tab => {
                tab.classList.remove('active');
            });
            
            // –ü–æ–∫–∞–∑–∞—Ç—å –≤—ã–±—Ä–∞–Ω–Ω—É—é —Å—Ç—Ä–∞–Ω–∏—Ü—É
            document.getElementById(tabName).classList.add('active');
            
            // –î–æ–±–∞–≤–∏—Ç—å –∞–∫—Ç–∏–≤–Ω—ã–π –∫–ª–∞—Å—Å –∫ –≤—ã–±—Ä–∞–Ω–Ω–æ–π –≤–∫–ª–∞–¥–∫–µ
            document.querySelector(`[data-page="${tabName}"]`).classList.add('active');
            
            // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∏—Ä–æ–≤–∞—Ç—å –≥—Ä–∞—Ñ–∏–∫–∏ –¥–ª—è —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É—é—â–∏—Ö —Å—Ç—Ä–∞–Ω–∏—Ü
            setTimeout(() => {
                if (tabName === 'pipeline') {
                    initPipelineChart();
                } else if (tabName === 'trends') {
                    initEngagementChart();
                    initResponseTimeChart();
                }
            }, 100);
        }
        
        // –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ —Å–æ–±—ã—Ç–∏–π –¥–ª—è –≤–∫–ª–∞–¥–æ–∫
        document.querySelectorAll('.nav-tab').forEach(tab => {
            tab.addEventListener('click', () => {
                const pageName = tab.getAttribute('data-page');
                switchTab(pageName);
            });
        });
        
        // –ê–Ω–∏–º–∞—Ü–∏—è —Å—á–µ—Ç—á–∏–∫–æ–≤
        function animateCounter(element, target, duration = 2000) {
            const start = 0;
            const range = target - start;
            const startTime = performance.now();
            
            function updateCounter(currentTime) {
                const elapsedTime = currentTime - startTime;
                const progress = Math.min(elapsedTime / duration, 1);
                const easeOutQuart = 1 - Math.pow(1 - progress, 4);
                const current = Math.floor(start + (range * easeOutQuart));
                
                element.textContent = current;
                
                if (progress < 1) {
                    requestAnimationFrame(updateCounter);
                } else {
                    element.textContent = target;
                }
            }
            
            requestAnimationFrame(updateCounter);
        }
        
        // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –∞–Ω–∏–º–∞—Ü–∏–∏ —Å—á–µ—Ç—á–∏–∫–æ–≤
        function initCounters() {
            document.querySelectorAll('[data-target]').forEach(element => {
                const target = parseInt(element.getAttribute('data-target'));
                animateCounter(element, target);
            });
        }
        
        // –ì—Ä–∞—Ñ–∏–∫ –≤–æ—Ä–æ–Ω–∫–∏ –ø—Ä–æ–¥–∞–∂
        function initPipelineChart() {
            const data = [{
                type: 'funnel',
                y: ['Lead', 'Contacted', 'Pitched', 'Due Diligence', 'Negotiation', 'Closed'],
                x: [221, 187, 134, 89, 45, 33],
                textinfo: 'value+percent initial',
                textposition: 'inside',
                marker: {
                    color: ['#667eea', '#7c8ce8', '#929ae6', '#a8a8e4', '#beb6e2', '#d4c4e0'],
                    line: {
                        width: 2,
                        color: 'white'
                    }
                },
                connector: {
                    line: {
                        color: '#667eea',
                        dash: 'solid',
                        width: 3
                    }
                }
            }];
            
            const layout = {
                title: {
                    text: '',
                    font: { size: 16, color: '#2d3748' }
                },
                margin: { l: 130, r: 130, t: 50, b: 50 },
                paper_bgcolor: 'rgba(0,0,0,0)',
                plot_bgcolor: 'rgba(0,0,0,0)',
                font: { family: 'Roboto, sans-serif', color: '#2d3748' }
            };
            
            const config = {
                responsive: true,
                displayModeBar: false
            };
            
            Plotly.newPlot('pipelineChart', data, layout, config);
        }
        
        // –ì—Ä–∞—Ñ–∏–∫ —Ç—Ä–µ–Ω–¥–æ–≤ –≤–æ–≤–ª–µ—á–µ–Ω–Ω–æ—Å—Ç–∏
        function initEngagementChart() {
            const dates = ['2024-01-01', '2024-01-08', '2024-01-15', '2024-01-22', '2024-01-29', '2024-02-05', '2024-02-12'];
            const hotData = [25, 28, 35, 38, 42, 45, 42];
            const warmData = [45, 48, 52, 58, 63, 67, 67];
            const coldData = [95, 92, 90, 89, 88, 87, 89];
            const inactiveData = [30, 28, 25, 24, 23, 22, 23];
            
            const data = [
                {
                    x: dates,
                    y: hotData,
                    type: 'scatter',
                    mode: 'lines+markers',
                    name: 'üî• Hot Prospects',
                    line: { color: '#ff6b6b', width: 3 },
                    marker: { size: 8, color: '#ff6b6b' }
                },
                {
                    x: dates,
                    y: warmData,
                    type: 'scatter',
                    mode: 'lines+markers',
                    name: 'üü† Warm Leads',
                    line: { color: '#ffa500', width: 3 },
                    marker: { size: 8, color: '#ffa500' }
                },
                {
                    x: dates,
                    y: coldData,
                    type: 'scatter',
                    mode: 'lines+markers',
                    name: 'üßä Cold Contacts',
                    line: { color: '#54a0ff', width: 3 },
                    marker: { size: 8, color: '#54a0ff' }
                },
                {
                    x: dates,
                    y: inactiveData,
                    type: 'scatter',
                    mode: 'lines+markers',
                    name: 'üí§ Inactive',
                    line: { color: '#a4b0be', width: 3 },
                    marker: { size: 8, color: '#a4b0be' }
                }
            ];
            
            const layout = {
                title: '',
                xaxis: { 
                    title: '–î–∞—Ç–∞',
                    gridcolor: '#e2e8f0',
                    font: { color: '#4a5568' }
                },
                yaxis: { 
                    title: '–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ –∏–Ω–≤–µ—Å—Ç–æ—Ä–æ–≤',
                    gridcolor: '#e2e8f0',
                    font: { color: '#4a5568' }
                },
                margin: { l: 60, r: 40, t: 40, b: 60 },
                paper_bgcolor: 'rgba(0,0,0,0)',
                plot_bgcolor: 'rgba(0,0,0,0)',
                font: { family: 'Roboto, sans-serif', color: '#2d3748' },
                legend: { 
                    orientation: 'h',
                    y: -0.2,
                    x: 0.5,
                    xanchor: 'center'
                }
            };
            
            const config = {
                responsive: true,
                displayModeBar: false
            };
            
            Plotly.newPlot('engagementChart', data, layout, config);
        }
        
        // –ì—Ä–∞—Ñ–∏–∫ –≤—Ä–µ–º–µ–Ω–∏ –æ—Ç–∫–ª–∏–∫–∞
        function initResponseTimeChart() {
            const data = [{
                x: ['–ü–Ω', '–í—Ç', '–°—Ä', '–ß—Ç', '–ü—Ç', '–°–±', '–í—Å'],
                y: [3.2, 2.1, 2.8, 1.9, 2.4, 4.1, 3.8],
                type: 'bar',
                marker: {
                    color: ['#667eea', '#7c8ce8', '#929ae6', '#a8a8e4', '#beb6e2', '#d4c4e0', '#eae2de'],
                    line: {
                        color: 'white',
                        width: 2
                    }
                }
            }];
            
            const layout = {
                title: '',
                xaxis: { 
                    title: '–î–µ–Ω—å –Ω–µ–¥–µ–ª–∏',
                    font: { color: '#4a5568' }
                },
                yaxis: { 
                    title: '–ß–∞—Å—ã',
                    font: { color: '#4a5568' }
                },
                margin: { l: 50, r: 30, t: 30, b: 50 },
                paper_bgcolor: 'rgba(0,0,0,0)',
                plot_bgcolor: 'rgba(0,0,0,0)',
                font: { family: 'Roboto, sans-serif', color: '#2d3748' }
            };
            
            const config = {
                responsive: true,
                displayModeBar: false
            };
            
            Plotly.newPlot('responseTimeChart', data, layout, config);
        }
        
        // –§–∏–ª—å—Ç—Ä–∞—Ü–∏—è —Ç–∞–±–ª–∏—Ü—ã
        function filterTable(segment) {
            const rows = document.querySelectorAll('#investorsTableBody tr');
            
            rows.forEach(row => {
                if (segment === 'all' || row.getAttribute('data-segment') === segment) {
                    row.style.display = '';
                } else {
                    row.style.display = 'none';
                }
            });
            
            // –û–±–Ω–æ–≤–∏—Ç—å –∞–∫—Ç–∏–≤–Ω—ã–π —Ñ–∏–ª—å—Ç—Ä
            document.querySelectorAll('.filter-btn').forEach(btn => {
                btn.classList.remove('active');
            });
            document.querySelector(`[data-filter="${segment}"]`).classList.add('active');
        }
        
        // –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ —Ñ–∏–ª—å—Ç—Ä–æ–≤
        document.querySelectorAll('.filter-btn').forEach(btn => {
            btn.addEventListener('click', () => {
                const filter = btn.getAttribute('data-filter');
                filterTable(filter);
            });
        });
        
        // –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ –∫–ª–∏–∫–æ–≤ –ø–æ –∫–∞—Ä—Ç–æ—á–∫–∞–º —Å–µ–≥–º–µ–Ω—Ç–æ–≤
        document.querySelectorAll('.segment-card').forEach(card => {
            card.addEventListener('click', () => {
                const segment = card.getAttribute('data-segment');
                switchTab('table');
                setTimeout(() => {
                    filterTable(segment);
                }, 200);
            });
        });
        
        // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ —Å—Ç—Ä–∞–Ω–∏—Ü—ã
        document.addEventListener('DOMContentLoaded', () => {
            initCounters();
            
            // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∏—Ä–æ–≤–∞—Ç—å –≥—Ä–∞—Ñ–∏–∫–∏ —á–µ—Ä–µ–∑ –Ω–µ–±–æ–ª—å—à—É—é –∑–∞–¥–µ—Ä–∂–∫—É
            setTimeout(() => {
                if (document.getElementById('dashboard').classList.contains('active')) {
                    // –ì—Ä–∞—Ñ–∏–∫–∏ –¥–ª—è –≥–ª–∞–≤–Ω–æ–π —Å—Ç—Ä–∞–Ω–∏—Ü—ã –º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å –∑–¥–µ—Å—å –ø—Ä–∏ –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ—Å—Ç–∏
                }
            }, 500);
        });
        
        // –î–æ–±–∞–≤–ª—è–µ–º –ø–ª–∞–≤–Ω—É—é –ø—Ä–æ–∫—Ä—É—Ç–∫—É –ø—Ä–∏ –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏–∏ –≤–∫–ª–∞–¥–æ–∫
        function smoothScrollToTop() {
            window.scrollTo({
                top: 0,
                behavior: 'smooth'
            });
        }
        
        // –û–±–Ω–æ–≤–ª—è–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ –≤–∫–ª–∞–¥–æ–∫ –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è –ø—Ä–æ–∫—Ä—É—Ç–∫–∏
        document.querySelectorAll('.nav-tab').forEach(tab => {
            tab.addEventListener('click', () => {
                smoothScrollToTop();
            });
        });
    </script>
</body>
